{
  "name": "deep-reach",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "deep-reach": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "postbuild": "chmod +x dist/index.js",
    "prepublishOnly": "npm run build",
    "dev": "node --import tsx src/cli/index.ts run --profile .deep-reach",
    "send": "node --import tsx src/cli/index.ts send",
    "clean": "rm -rf node_modules dist .langgraph_data runs"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@langchain/anthropic": "^1.3.13",
    "@langchain/community": "^1.1.10",
    "@langchain/core": "^1.1.18",
    "@langchain/langgraph": "^1.1.2",
    "@langchain/openai": "^1.2.4",
    "@langchain/tavily": "^1.2.0",
    "commander": "^12.0.0",
    "consola": "^3.4.2",
    "deepagents": "^1.6.0",
    "dotenv": "^16.4.0",
    "googleapis": "^171.1.0",
    "langchain": "^1.2.16",
    "nodemailer": "^8.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.9",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3"
  }
}
